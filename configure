    _def_alsa='#define CONFIG_ALSA 1'
    _def_alsa1x="#define CONFIG_ALSA1X 1"
    def_alsa_asoundlib_h='#define HAVE_ALSA_ASOUNDLIB_H 1'
    _res_comment="using alsa 1.0.x and alsa/asoundlib.h"
